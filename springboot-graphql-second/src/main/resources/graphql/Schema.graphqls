type Query{
    getAllMoviesHandler: [Movie]
}

type Mutation{
    addMovieHandler(movieInput: MovieInput): Movie
}

type Movie{
    id: ID!
    title: String!
    director: String!
    studio: String!
    releaseYear: Int!
    movieCast: [String]!
}

input MovieInput{
    title: String!
    director: String!
    studio: String!
    releaseYear: Int!
    movieCast: [String]!
}