type Query{
    movies: [Movie]
}

type Mutation{
    addMovieHandler(movieInput: MovieInput): Movie
    updateMovieHandler(id: Int, movieInput: MovieInput): Movie
    deleteMovieHandler(id: Int): String
}

type Movie{
    id: ID!
    title: String!
    director: String!
    studio: String!
    releaseYear: Int!
    movieCast: [String]!
}

input MovieInput{
    title: String!
    director: String!
    studio: String!
    releaseYear: Int!
    movieCast: [String]!
}